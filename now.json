{
  "name": "symfony-demo",
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "now-php@0.0.9"
    }
  },
  "routes": [
    { "src": "/(.*)",  "dest": "/api/index.php" }
  ],
  "env": {
    "PROJECT_NAME": "evidapp-backend",
    "APP_ENV": "prod",
    "APP_SECRET": "@evidapp-backend.app-secret",
    "NOW_DEPLOYMENT": "fukthishacking",
    "DATABASE_URL": "@evidapp-backend.db-url",
    "MAILER_URL": "null://localhost",
    "JWT_SECRET_KEY": "@evidapp-backend.jwt-passphrase",
    "CORS_ALLOW_ORIGIN": "^https?://localhost(:[0-9]+)?$"
  },
  "public": true
}